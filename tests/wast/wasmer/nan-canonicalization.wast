;; Test canonicalization is done before branch in `else` operator.
(module
  (func (;0;)
    (local f64)
    i32.const 1
    if (result f64)
      local.get 0
      f64.const 0x1p+0 (;=1;)
      f64.add
    else
      f64.const 0x0p+0 (;=0;)
    end
    return
  )
)

;; Test canonicalization is done before branch in `return` operator.
(module
  (func (;0;) (result f64)
    (local f64)
    f64.const 0x0p+0
    local.get 0
    f64.mul
    return
  )
)
